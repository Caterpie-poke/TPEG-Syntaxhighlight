{
    "name": "tpeg-syntax",
    "displayName": "TPEG Syntax Highlight",
    "description": "Syntax highlight of TPEG",
    "publisher": "ywata",
    "version": "0.2.5",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Caterpie-poke/TPEG-Syntaxhighlight"
    },
    "icon": "images/pegtree.png",
    "contributes": {
        "languages": [
            {
                "id": "tpeg",
                "aliases": [
                    "TPEG",
                    "tpeg"
                ],
                "extensions": [
                    ".tpeg",
                    ".gpeg",
                    ".pegtree"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tpeg",
                "scopeName": "source.tpeg",
                "path": "./syntaxes/tpeg.tmLanguage.json"
            }
        ]
    }
}
